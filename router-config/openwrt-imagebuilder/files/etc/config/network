
config interface 'loopback'
	option device 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option packet_steering '1'

config interface 'lan'
	option proto 'dhcp'
	option force_link '1'
	option delegate '0'
	option device 'br-lan'

config device
	option name 'eth0'
	option acceptlocal '1'
	option ipv6 '0'
	option multicast '0'

config device
	option type 'bridge'
	option name 'br-lan'
	option bridge_empty '1'
	option stp '1'
	option igmp_snooping '1'
	option acceptlocal '1'
	option ipv6 '0'
	option multicast '0'
	list ports 'eth0'

config interface 'docker'
	option device 'docker0'
	option proto 'none'
	option auto '0'

config device
	option type 'bridge'
	option name 'docker0'
	list ports 'vetha47d15f'
	list ports 'vethf467816'
	option stp '1'
	option igmp_snooping '1'
	option acceptlocal '1'

