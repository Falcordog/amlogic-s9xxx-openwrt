
config interface 'loopback'
	option device 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option packet_steering '1'

config interface 'lan'
	option proto 'dhcp'
	option force_link '1'
	option delegate '0'
	option device 'br-lan'

config device
	option name 'eth0'
	option acceptlocal '1'
	option ipv6 '0'
	option multicast '0'
	option promisc '1'

config interface 'libernet'
	option proto 'none'
	option device 'tun1'
	option auto '0'

config device
	option type 'veth'
	option name 'tun1'
	option ipv6 '0'

config device
	option name 'br-9c46ec928349'
	option type 'bridge'
	list ports 'vethdfc99c0'
	list ports 'vetheb7cc6f'
	option bridge_empty '1'
	option multicast '0'
	option stp '1'
	option igmp_snooping '1'
	option acceptlocal '1'

